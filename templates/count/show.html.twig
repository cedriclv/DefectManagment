{% extends 'base.html.twig' %}

{% block title %}Count{% endblock %}

{% block body %}
    <h1>Count</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ count.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ count.date ? count.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_count_index') }}">back to list</a>

    <a href="{{ path('app_count_edit', {'id': count.id}) }}">edit</a>

    {{ include('count/_delete_form.html.twig') }}
{% endblock %}
